# PostgreSQL configuration
MAJORDOMO_STORAGE_POSTGRES_HOST=localhost
MAJORDOMO_STORAGE_POSTGRES_PORT=5432
MAJORDOMO_STORAGE_POSTGRES_USER=majordomo
MAJORDOMO_STORAGE_POSTGRES_PASSWORD=your-postgres-password
MAJORDOMO_STORAGE_POSTGRES_DATABASE=majordomo
MAJORDOMO_STORAGE_POSTGRES_SSLMODE=disable

# S3 configuration (required when s3.enabled=true)
MAJORDOMO_S3_ACCESS_KEY_ID=your-aws-access-key
MAJORDOMO_S3_SECRET_ACCESS_KEY=your-aws-secret-key

# Secrets configuration (required for proxy keys)
# Generate with: openssl rand -hex 32
MAJORDOMO_SECRETS_ENCRYPTION_KEY=
